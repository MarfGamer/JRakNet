<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
  <Appenders>
    <Console name="RakNetConsole" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss} [%level] %logger{36} %msg%n" />
    </Console>
    <File name="RakNetLog" fileName="raknet.log" immediateFlush="false" append="false">
       <PatternLayout pattern="%d{HH:mm:ss} [%level] %logger{36} %msg%n" />
    </File>
  </Appenders>
  <Loggers>
    <Root level="debug">
     <AppenderRef ref="RakNetConsole" />
     <AppenderRef ref="RakNetLog"/>
    </Root>
  </Loggers>
</Configuration>